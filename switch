let x = Number()
switch((x / 4 == 0) ? 0 : 1){
    case 0:
        console.log("leap year")
        break ;
    case 1:
        console.log("not a leap year")
        break;
}


let d = Number(prompt())
switch((d % 4 == 0)? 0 : 1){
    case 0:
        console.log("leap year")
        break ;
    case 1:
        console.log("not A leap year")
        break ;
}
